<h1>Listing places</h1>

<table>
  <thead>
    <tr>
      <th>Business</th>
      <th>Area</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.business %></td>
        <td><%= place.area %></td>
        <td><%= link_to 'Show', place %></td>
        <td><%= link_to 'Edit', edit_place_path(place) %></td>
        <td><%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= "@json" %>
<br>

<%= "@data_page" %>

<%= "@arr" %>
<br/>

<% @pages_info.each do |p| %>
  <% @page = p.second %>
  <%= @page["id"] %> 
  <%= @page["name"] %>
  <%= @page["category"] %>
  <%= @page["description"] %>
  <%= @page["website"] %>
  <%= @page["phone"] %>
  <%= @page["location"]["street"] %>, <%= @page["location"]["city"] %>
  <%= @page["likes"] %>
  <%= @page["about"] %>
  <%= @page["link"] %>

  <br/>
  <br/>
<% end %>

<br/>

<%= link_to 'New Place', new_place_path %>